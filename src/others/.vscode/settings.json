{
  "cSpell.enabled": false,

  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.venv": true,
    ".ruff_cache": true,
    "*.lock": true
  },
  "explorerExclude.backup": {},

  "tinymist.formatterMode": "typstyle",
  "[typst]": {
    "editor.formatOnSave": true
  }
}
