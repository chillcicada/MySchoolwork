{
  "clangd.arguments": [
    "--compile-commands-dir=.vscode"
  ],
  "xmake.debugConfigType": "codelldb",

  "cSpell.enabled": false,

  "doxdocgen.generic.useGitUserName": false,
  "doxdocgen.generic.useGitUserEmail": false,
  "doxdocgen.file.versionTag": "@version 0.0.0",
  "doxdocgen.generic.authorTag": "@author liukuan (liukuan22@mails.tsinghua.edu.cn)",
  "doxdocgen.file.copyrightTag": [
    "@copyright Copyright (C) {year} MIT License"
  ],

  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.venv": true,
    ".direnv": true,
    "**/.cache": true,
    ".xmake": true
  },
  "explorerExclude.backup": {}
}
