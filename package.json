{
  "name": "cc-schoolwork",
  "displayName": "Cc Schoolwork",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "chillcicada's schoolwork",
  "author": "chillcicada",
  "license": "MIT",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "preview": "vitepress preview docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "vue": "3.4.31"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.22.2",
    "@shikijs/vitepress-twoslash": "1.10.3",
    "@types/node": "20.14.10",
    "eslint": "9.7.0",
    "eslint-plugin-format": "0.1.2",
    "lint-staged": "15.2.7",
    "markdown-it-mathjax3": "4.3.2",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.5.3",
    "vitepress": "1.3.1",
    "vue-tsc": "2.0.26"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "docs/.vitepress/*": "eslint --fix",
    "*.{py,ipynb}": [
      "ruff check --fix",
      "ruff format"
    ]
  }
}
